{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",   
    "paths": {
      "@env": ["./@types/env.d.ts"],
      "@/*": ["./src/*"],
      "@types/*": ["./src/utils/*"]
    }
  },
"include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/utils//**/*.d.ts",
  ],
}